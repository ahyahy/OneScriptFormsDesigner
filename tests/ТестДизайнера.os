Функция ЗаполнитьМассивСвойств(ИмяТипаКонтрола)
	Стр = "";
	Если ИмяТипаКонтрола = "ВертикальнаяПрокрутка" Тогда
		Для А = 0 По МассивСвойствВертикальнаяПрокрутка.Количество() - 1 Цикл	
			Стр = Стр + "МассивСвойств.Добавить(""" + МассивСвойствВертикальнаяПрокрутка.Получить(А).Значение + """);"  + Символы.ПС;
		КонецЦикла;
	ИначеЕсли ИмяТипаКонтрола = "Вкладка" Тогда
		Для А = 0 По МассивСвойствВкладка.Количество() - 1 Цикл	
			Стр = Стр + "МассивСвойств.Добавить(""" + МассивСвойствВкладка.Получить(А).Значение + """);"  + Символы.ПС;
		КонецЦикла;
	ИначеЕсли ИмяТипаКонтрола = "ГлавноеМеню" Тогда
		Для А = 0 По МассивСвойствГлавноеМеню.Количество() - 1 Цикл	
			Стр = Стр + "МассивСвойств.Добавить(""" + МассивСвойствГлавноеМеню.Получить(А).Значение + """);"  + Символы.ПС;
		КонецЦикла;
