Функция ОтобратьФайлы(Фильтр)
	// Фильтр = Класс Конструктор Члены Свойства Свойство Методы Метод Перечисление События
	М_Фильтр = Новый Массив;
	ВыбранныеФайлы = НайтиФайлы(КаталогСправки, "*.html", Истина);
	Найдено1 = 0;
	Для А = 0 По ВыбранныеФайлы.ВГраница() Цикл
		ТекстДок = Новый ТекстовыйДокумент;
		ТекстДок.Прочитать(ВыбранныеФайлы[А].ПолноеИмя);
		Стр = ТекстДок.ПолучитьТекст();
		МассивСписок1 = Ф.СтрНайтиМежду(Стр, "<H1 class=dtH1", "/H1>", , );
		Если МассивСписок1.Количество > 0 Тогда
			СтрЗаголовка= МассивСписок1.Элемент(0);
			Если (СтрНайти(СтрЗаголовка, Фильтр + "<") > 0) или (СтрНайти(СтрЗаголовка, Фильтр + " <") > 0) Тогда
				Найдено1 = Найдено1 + 1;
				М_Фильтр.Добавить(ВыбранныеФайлы[А].ПолноеИмя);
			КонецЕсли;
		КонецЕсли;
	КонецЦикла;
	
	Сообщить("Найдено1 (" + Фильтр + ") = " + Найдено1);
	Возврат М_Фильтр;
КонецФункции

Функция СоздатьСтрокуМассиваСобытий();
	Стр = "М_Событий = Новый Массив();// Массив имен событий." + Символы.ПС;
	М_Событий = Новый Массив();// Массив имен событий.
	ОтобранныеСобытия = ОтобратьФайлы("События");// Из справки отберем файлы с событиями.
	Для А = 0 По ОтобранныеСобытия.ВГраница() Цикл
		ТекстДок = Новый ТекстовыйДокумент;
		ТекстДок.Прочитать(ОтобранныеСобытия[А]);
		Стр1 = ТекстДок.ПолучитьТекст();
		МассивСписок1 = Ф.СтрНайтиМежду(Стр1, "<TD width=", "</TD>", , );
		Если МассивСписок1.Количество > 0 Тогда
			Для А1 = 0 По МассивСписок1.Количество - 1 Цикл
				СтрСобытия = МассивСписок1.Элемент(А1);
				МассивСписок2 = Ф.СтрНайтиМежду(СтрСобытия, ".html"">", "(", , );
				Если МассивСписок2.Количество > 0 Тогда
					СтрСобытия = МассивСписок2.Элемент(0);
					СтрСобытия = СтрЗаменить(СтрСобытия, "&nbsp;", "");
					Если М_Событий.Найти(СтрСобытия) = Неопределено Тогда
						М_Событий.Добавить(СтрСобытия);
					КонецЕсли;
				КонецЕсли;
			КонецЦикла;
		КонецЕсли;
	КонецЦикла;
	
	Для А = 0 По М_Событий.ВГраница() Цикл
		Стр = Стр + "М_Событий.Добавить(""" + М_Событий[А] + """);" + Символы.ПС;
	КонецЦикла;
	Возврат СокрЛП(Стр);
КонецФункции//СоздатьСтрокуМассиваСобытий

