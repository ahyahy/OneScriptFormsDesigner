	ПутьФайла = ОбъединитьПути(ТекПуть,"ФайлыТеста\Выгрузка0.osd");
	ФайлНаДиске = Новый Файл(ПутьФайла);
	Если Не ФайлНаДиске.Существует() Тогда
		Стр1 = "
		|[<Конструкторы]
		|Форма_0 = Ф.Форма();
		|[Конструкторы>]
		|[<Свойства]
		|[<Форма_0]
		|Форма_0.Размер = Ф.Размер(670, 600);
		|Форма_0.Путь = """ + ПутьФайла + """;
		|Форма_0.Текст = ""Форма_0"";
		|[Форма_0>]
		|[Свойства>]
		|";
		ТекстДок5 = Новый ТекстовыйДокумент;
		ТекстДок5.УстановитьТекст(Стр1);
		ТекстДок5.Записать(ПутьФайла, "UTF-8");
		Приостановить(1000);
	КонецЕсли;
КонецПроцедуры

