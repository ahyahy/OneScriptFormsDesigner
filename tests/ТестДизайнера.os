		|	ЗначениеСвойства = ДФ.ПолучитьЗначениеСвойства(ИмяКонтрола, ИмяСвойства);
		|	Сообщить(""ТипСтрокой = "" + ТипСтрокой);
		|	// ======================================================
		|	// Для событий формируем строковое значение.
		|	Если Не (М_Событий.Найти(ИмяСвойства) = Неопределено) Тогда
		|		Возврат ИмяКонтрола + ""_"" + ИмяСвойства;
		|	// ======================================================
		|	// Для булевых.
		|	ИначеЕсли ТипСтрокой = ""System.Boolean"" Тогда
		|		Возврат Не ЗначениеСвойства;
		|	// ======================================================
		|	// Для цвета обработка происходит в коде библиотеки по условию: тип - строка и имени свойства.
		|	ИначеЕсли ТипСтрокой = ""System.Drawing.Color"" Тогда
		|		Возврат ""System.Drawing.Color"";
		|	// ======================================================
		|	// Для дат.
